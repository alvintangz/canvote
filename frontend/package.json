{
  "name": "canvote-frontend",
  "version": "1.0.0",
  "description": "Frontend for CanVote, a CSCC09 Project.",
  "author": "Dharmik Shah; Alvin Tang; Mikhail Makarov",
  "private": true,
  "scripts": {
    "build-css": "./node_modules/node-sass/bin/node-sass styles/ -o public/styles",
    "watch-css": "yarn build-css & ./node_modules/node-sass/bin/node-sass styles/ -o public/styles --watch --recursive",
    "clean-css": "node ./node_modules/rimraf/bin.js public/styles",
    "start-react-scripts": "node ./node_modules/dotenv-cli/cli.js -e .env.development react-scripts start",
    "build-react-scripts": "react-scripts build",
    "start": "npm-run-all -p watch-css start-react-scripts",
    "build": "npm-run-all -p build-css build-react-scripts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.41",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "apollo-boost": "^0.4.7",
    "axios": "^0.19.2",
    "bootstrap-sass": "3.4.1",
    "dotenv-cli": "^3.1.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "graphql.macro": "^1.4.2",
    "leaflet": "^1.6.0",
    "node-sass": "^4.13.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.13.0",
    "react-leaflet": "^2.6.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-simple-code-editor": "^0.11.0",
    "redux": "^4.0.5",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "express-conditional-middleware": "^2.1.1",
    "http-proxy-middleware": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
